{
  "name": "minerva-geojson",
  "version": "0.1.0",
  "description": "HTML widgets for applying styles to GeoJSON features.",
  "main": "dist/minerva-geojson.js",
  "scripts": {
    "build": "npm run clean && npm run build:dev && npm run build:prd",
    "build:dev": "webpack --config tools/webpack.config.dev.js",
    "build:prd": "webpack --config tools/webpack.config.prd.js",
    "prepublish": "npm run build",
    "clean": "rimraf dist build",
    "lint": "eslint --cache .",
    "test": "npm run lint && karma start --single-run --no-auto-watch karma.config.js",
    "codecov": "codecov"
  },
  "keywords": [
    "geojs",
    "minerva",
    "geojson"
  ],
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.3",
    "babel-istanbul": "^0.8.0",
    "babel-istanbul-loader": "^0.1.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.6.0",
    "codecov": "^1.0.1",
    "eslint": "^2.8.0",
    "eslint-config-semistandard": "^6.0.1",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.22",
    "karma-coverage": "^0.5.5",
    "karma-jasmine": "^0.3.8",
    "karma-jasmine-html-reporter": "^0.2.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-threshold-reporter": "^0.1.15",
    "karma-webpack": "^1.7.0",
    "loader-utils": "^0.2.14",
    "phantomjs-prebuilt": "^2.1.7",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "webpack": "^1.13.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.6",
    "d3": "^3.5.16",
    "jquery": "^2.1.1",
    "lodash": "^4.11.1"
  }
}
